{
    // Configurações de chaves de API para os diferentes provedores de IA
    "api_keys": {
        "gemini": "sua-chave-gemini", // Chave de API do Google Gemini
        "openai": "sua-chave-openai", // Chave de API do OpenAI
        "anthropic": "sua-chave-anthropic" // Chave de API do Anthropic
    },
    // Configurações específicas para cada modelo de IA
    "models": {
        "gemini": {
            "temperature": 0.7, // Controla a criatividade (0.0 a 1.0)
            "top_k": 40, // Número de tokens considerados
            "top_p": 0.95 // Probabilidade cumulativa
        },
        "openai": {
            "model": "gpt-4", // Modelo a ser utilizado
            "temperature": 0.7, // Controla a criatividade (0.0 a 1.0)
            "max_tokens": 2000 // Número máximo de tokens na resposta
        },
        "anthropic": {
            "model": "claude-2", // Modelo a ser utilizado
            "temperature": 0.7, // Controla a criatividade (0.0 a 1.0)
            "max_tokens": 2000 // Número máximo de tokens na resposta
        }
    },
    // Configurações gerais dos agentes
    "agents": {
        "max_concurrent_tasks": 5, // Número máximo de tarefas simultâneas
        "task_timeout": 300, // Tempo máximo em segundos por tarefa
        "memory_size": 1000, // Tamanho máximo da memória do agente
        "max_retries": 3 // Número máximo de tentativas
    },
    // Configurações do sistema de memória
    "memory": {
        "max_size": 10000, // Tamanho máximo da memória em tokens
        "ttl": 3600, // Tempo de vida em segundos
        "cleanup_interval": 300, // Intervalo de limpeza em segundos
        "cache_size": 1000 // Tamanho do cache em tokens
    },
    // Configurações do sistema de comunicação
    "communication": {
        "max_messages": 1000, // Número máximo de mensagens armazenadas
        "message_ttl": 3600, // Tempo de vida das mensagens em segundos
        "priority_levels": ["high", "medium", "low"] // Níveis de prioridade
    },
    // Configurações da fusão de contexto
    "context_fusion": {
        "max_contexts": 10, // Número máximo de contextos
        "context_ttl": 3600 // Tempo de vida do contexto em segundos
    },
    // Configurações do workflow
    "workflow": {
        "max_agents": 10, // Número máximo de agentes
        "max_tasks": 100, // Número máximo de tarefas
        "timeout": 3600, // Tempo máximo em segundos
        "retry_attempts": 3 // Número de tentativas
    },
    // Configurações das integrações
    "integrations": {
        "slack": {
            "bot_token": "seu-token-slack", // Token do bot do Slack
            "app_token": "seu-app-token-slack", // Token do app do Slack
            "channel_id": "seu-channel-id" // ID do canal
        },
        "github": {
            "token": "seu-token-github", // Token de acesso do GitHub
            "repo_owner": "seu-usuario", // Proprietário do repositório
            "repo_name": "seu-repositorio" // Nome do repositório
        },
        "jira": {
            "server": "https://seu-jira.atlassian.net", // URL do servidor Jira
            "email": "seu-email", // Email de acesso
            "api_token": "seu-token-jira" // Token de API do Jira
        },
        "discord": {
            "token": "seu-token-discord", // Token do bot do Discord
            "guild_id": "seu-guild-id" // ID do servidor
        }
    }
} 